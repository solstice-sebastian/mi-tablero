{{#paper-content class="md-whiteframe-z1 list-demo"}}
  {{#paper-list}}
    {{#each model as |activeAsset|}}
      {{#paper-item}}
        <div class="md-list-item-text">
          <h3>{{activeAsset.id}}</h3>
          <h4>Last buy in: {{activeAsset.lastBuyIn.price}}</h4>
          <p>
            Current price: {{activeAsset.currentPrice}}
          </p>
          <p>
            Current P/L: {{pretty-percent activeAsset.currentProfitLoss}}
          </p>
        </div>
      {{/paper-item}}
      {{paper-divider}} {{!-- OR {{paper-divider inset=true}} --}}
    {{/each}}
  {{/paper-list}}
{{/paper-content}}